<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.myapplication.myapplication.QuestionairreActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="205dp"
        android:layout_height="118dp"
        android:text="How are you feeling today?"
        android:gravity="center"
        android:textSize="24dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/seekBar1"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.503" />

    <SeekBar
        android:id="@+id/seekBar1"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="296dp"
        android:layout_height="17dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_margin="10dp"
        android:indeterminate="false"
        android:max="2"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:progress="1"
        android:progressDrawable="@drawable/my_progressbar_style"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.447" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="98dp"
        android:layout_height="50dp"
        android:text="Very Bad"
        android:gravity="left"
        android:textSize="18dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="277dp"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="98dp"
        android:layout_height="50dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="277dp"
        android:gravity="right"
        android:text="Very Good"
        android:textSize="18dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="239dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="98dp"
        android:layout_height="50dp"
        android:layout_marginLeft="125dp"
        android:layout_marginRight="125dp"
        android:layout_marginTop="277dp"
        android:gravity="center|top"
        android:text="Meh"
        android:textSize="18dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/imageButton3"
        android:layout_width="125dp"
        android:layout_height="75dp"
        android:layout_marginBottom="43dp"
        android:layout_marginLeft="39dp"
        android:text="Previous Question"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="485dp" />

    <Button
        android:id="@+id/imageButton4"
        android:layout_width="125dp"
        android:layout_height="75dp"
        android:layout_marginBottom="43dp"
        android:layout_marginRight="39dp"
        android:text="Next Question"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteX="253dp"
        tools:layout_editor_absoluteY="485dp" />

</android.support.constraint.ConstraintLayout>
